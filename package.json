{
  "name": "vscode-filamentphp-helpers",
  "displayName": "Filament PHP Helpers",
  "description": "Filament PHP Helpers",
  "icon": "assets/icon.png",
  "version": "0.4.0",
  "publisher": "JuniorPeixoto",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/juniorpeixoto2/vscode-filamentphp-helpers"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "make.resource",
        "title": "Filament - Make Resource"
      },
      {
        "command": "make.widget",
        "title": "Filament - Make Widget"
      },
      {
        "command": "make.custom.page",
        "title": "Filament - Make Custom Page"
      },
      {
        "command": "make.panel",
        "title": "Filament - Make Panel"
      },
      {
        "command": "make.theme",
        "title": "Filament - Make Theme"
      },
      {
        "command": "make.relation.manager",
        "title": "Filament - Make Relation Manager"
      },
      {
        "command": "make.livewire.component",
        "title": "Livewire - Make Component"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}
